[core]
	repositoryformatversion = 0
	filemode = false
	bare = false
	logallrefupdates = true
	symlinks = false
	ignorecase = true
[remote "origin-github"]
	url = git@github.com:lag-spike/lag-spike.github.io.git
	fetch = +refs/heads/*:refs/remotes/origin-github/*
[remote "origin"]
	url = git@github.com:lag-spike/lag-spike.github.io.git
	fetch = +refs/heads/*:refs/remotes/origin/*
[remote "origin-gitee"]
    url = git@gitee.com:lag-spike/blog_backup_gitee.git
    fetch = +refs/heads/*:refs/remotes/origin-gitee/*
[remote "origin-all"]
	url = git@github.com:lag-spike/lag-spike.github.io.git
	fetch = +refs/heads/*:refs/remotes/origin-all/*
	pushurl = git@gitee.com:lag-spike/blog_backup_gitee.git
	pushurl = git@github.com:lag-spike/lag-spike.github.io.git
